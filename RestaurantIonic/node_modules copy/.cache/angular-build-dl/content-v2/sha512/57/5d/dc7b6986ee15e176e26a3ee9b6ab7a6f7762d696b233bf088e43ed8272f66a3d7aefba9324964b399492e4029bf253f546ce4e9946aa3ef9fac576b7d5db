/* harmony import */ var src_app_utils_service__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! src/app/utils.service */ "./src/app/utils.service.ts");








let ModifierPlatPage = class ModifierPlatPage {
    constructor(route, service, formBuilder, nav, utils) {
        this.route = route;
        this.service = service;
        this.formBuilder = formBuilder;
        this.nav = nav;
        this.utils = utils;
        this.platId = Number(this.route.snapshot.paramMap.get('id'));
        this.service.getPlat(this.platId).subscribe(plat => {
            this.plat = plat;
            this.createForm();
        });
        console.log("Id=" + this.platId);
    }
    createForm() {
        this.nomControl = new _angular_forms__WEBPACK_IMPORTED_MODULE_5__["FormControl"](this.plat.nom, [_angular_forms__WEBPACK_IMPORTED_MODULE_5__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_5__["Validators"].minLength(2)]);
        this.prixControl = new _angular_forms__WEBPACK_IMPORTED_MODULE_5__["FormControl"](this.plat.prix, [_angular_forms__WEBPACK_IMPORTED_MODULE_5__["Validators"].required]);
        this.descriptionControl = new _angular_forms__WEBPACK_IMPORTED_MODULE_5__["FormControl"](this.plat.description);
        this.restaurantControl = new _angular_forms__WEBPACK_IMPORTED_MODULE_5__["FormControl"](this.plat.restaurant, [_angular_forms__WEBPACK_IMPORTED_MODULE_5__["Validators"].required]);
        this.formGroup = this.formBuilder.group({
            nom: this.nomControl,
            prix: this.prixControl,
            description: this.descriptionControl,
            restaurant: this.restaurantControl
        });
    }
    ngOnInit() {
        this.getRestaurants();
    }
    modifierPlat() {
        let infoPlat = new src_app_Models_plat__WEBPACK_IMPORTED_MODULE_4__["Plat"]();
        infoPlat.id = this.platId;
        infoPlat.nom = this.formGroup.get('nom').value;
        infoPlat.prix = this.formGroup.get('prix').value;
        infoPlat.description = this.formGroup.get('description').value;
        infoPlat.restaurant = this.formGroup.get('restaurant').value;
        console.log(this.formGroup.value);
        this.service.updatePlat(infoPlat).subscribe(plat => {
            this.utils.presentToast("Plat modifié avec succés", "success");
            this.nav.back();
            console.log(plat);
        }, error => {
            this.utils.presentToast("Une erreur est survenue", "danger");
        });
    }
    getRestaurants() {
        this.service.getRestaurants().subscribe(restaurant => {
            this.restaurants = restaurant;
        }, error => {
            this.utils.presentToast('Erreur survenue', 'danger');
        });
    }
};
